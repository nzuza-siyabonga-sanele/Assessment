{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Senior_Developer_Assessment",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Senior_Developer_Assessment/bin/Debug/net8.0/Senior_Developer_Assessment.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Senior_Developer_Assessment",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "DOTNET_URLS": "http://+:5000;https://+:5001",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "postDebugTask": "Open Swagger"
    }
  ]
}
